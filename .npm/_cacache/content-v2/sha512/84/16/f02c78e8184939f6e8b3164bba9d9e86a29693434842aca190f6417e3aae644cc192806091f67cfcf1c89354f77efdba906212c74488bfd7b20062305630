rMode: \"light\", // or \"dark\"\n    cssVarPrefix: \"chakra\", // any string\n  }\n}\n\n// 3. Use at the root of your app\nfunction App({ Component }) {\n  return (\n    <ChakraProvider theme={theme}>\n      <Component />\n    </ChakraProvider>\n  )\n}\n\n```\n","readmeFilename":"README.md","gitHead":"720158df0e7984b4e09421f4ef22d8bc5017921d","bugs":{"url":"https://github.com/chakra-ui/chakra-ui/issues"},"_id":"@chakra-ui/provider@0.0.0-pr-20211113195848","_nodeVersion":"16.13.1","_npmVersion":"8.1.2","dist":{"integrity":"sha512-Fkvz7r9yyUkknFBvSGqlSOXisxtawVUDgZPyTKWsTyS2G13d8pwORvmb9CtNWsiZmHLU1fYUw13ZPM5sP1iEQQ==","shasum":"578941c05c5d0bf5dfc0422649373c8cf427a531","tarball":"https://registry.npmjs.org/@chakra-ui/provider/-/provider-0.0.0-pr-20211113195848.tgz","fileCount":13,"unpackedSize":14278,"npm-signature":"-----BEGIN PGP SIGNATURE-----\r\nVersion: OpenPGP.js v3.0.13\r\nComment: https://openpgpjs.org\r\n\r\nwsFcBAEBCAAQBQJht6YVCRA9TVsSAnZWagAAtfsP/2/MnH/ihGi0pq7LMrKN\nMTd7nv7RsCZgC8j4Wnj7dl9JmChGvmaP7XIvG0VP/yS1fQ92njRQIyjrkJXs\nBtbxS1lwUeGjA522sIkfr29eb7ahePUSpIwPs4488MGVQSAcW1qU1j9cTlZL\nL1hVyEwFsEQCZgN+t8l8usVWINaKfSNlFadC4YVEOCnNZdhcUSzaAxyEJebs\nIQQBFDTLPpU0R1nrokAGoiV+tfvDPXaTc6iSJsJLnte95t1kZBFSE6KuRope\n3iKWvPv78I4Yt/p5TWhJeqbzajlyomO1FdTLm2dYUw/nMymfHqDj2HPadrzF\nODxzQmFXGdfsyFf/csUiFDmZO1lnfbIAuJjV9ePm2MPZMGkqBCjh/DQDhh/F\n+hXVHf0p5zzgdq6FWGRu/MF1gnhiskzuj1wJf0PBNY4EpoPEQgxElFCp6Eg3\nO0JsS53M/sgT/RXdQ4uEuCRAsRe6XdYDT1447FXj9Bk9aULLhOZiIDKKsNP/\nWoXLYFgLkOA/jjKtylCqCfEypFxvv/jYjuQPqRxAx2nUdheTbBzq5O72P8i3\nymJhOHIc+qdNVb+siqlt898dUKKbZBuMYT/yfd2Zi2RuhpaBA5R7iEdqv+0h\nPudBCvUdBjtNWVlpKTOd94hT/j8UfopiDnTZ53Lm60ud+U9h3ii0cRGpLV6o\n5Bb+\r\n=2Y5x\r\n-----END PGP SIGNATURE-----\r\n","signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEYCIQDJbcIIMTHzhRD2/MfTwWOj6AjUF77PZRzPWSnBAp4H2QIhAN3icFgpUKxtmLsk0VxaQpDfo2yOOVPvco4SAmRY3Nl1"}]},"_npmUser":{"name":"segunadebayo","email":"joseshegs@gmail.com"},"directories":{},"maintain