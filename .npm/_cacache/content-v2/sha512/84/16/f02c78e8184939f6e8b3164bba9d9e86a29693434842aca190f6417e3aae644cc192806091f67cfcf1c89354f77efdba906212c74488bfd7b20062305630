k\"\n    cssVarPrefix: \"chakra\", // any string\n  }\n}\n\n// 3. Use at the root of your app\nfunction App({ Component }) {\n  return (\n    <ChakraProvider theme={theme}>\n      <Component />\n    </ChakraProvider>\n  )\n}\n\n```\n","readmeFilename":"README.md","gitHead":"ebe0041b1dfa30714634b39bb57cdb590e929529","bugs":{"url":"https://github.com/chakra-ui/chakra-ui/issues"},"_id":"@chakra-ui/provider@0.0.0-dev-2022524225023","_nodeVersion":"16.15.1","_npmVersion":"8.11.0","dist":{"integrity":"sha512-Bbr4ikyxKF3aPzgW38tdBvHOjDbZ0XEDjgqjdG6wpNzARjfBVDDeJGMFWGC9q1D2jhTjAygABDMIs7GIZV4N3w==","shasum":"d0f3827e3584b0d617cba12613231c3d9929cd12","tarball":"https://registry.npmjs.org/@chakra-ui/provider/-/provider-0.0.0-dev-2022524225023.tgz","fileCount":11,"unpackedSize":11440,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEUCIQCfeKvrvwhGXM0IyQseMV3r2ea3Hm71VHl5zgXXy2+FKwIgL+2fBQAY8GWxeEKPkPNlSZeMjwdyiQSP5Txs37kp220="}],"npm-signature":"-----BEGIN PGP SIGNATURE-----\r\nVersion: OpenPGP.js v4.10.10\r\nComment: https://openpgpjs.org\r\n\r\nwsFzBAEBCAAGBQJitj/OACEJED1NWxICdlZqFiEECWMYAoorWMhJKdjhPU1b\r\nEgJ2VmqmGA/8C2f56M5TYuBqoIgVmrRkZ3xC6djGPa/pfiZHY/1pQHAUANFr\r\nS8p/JSVpNgF466idKsf/VpSGTddDjIzb1Ft11ea5SFEf6X+Yt7hVXZpOmcXG\r\nBH7c6AL0of7pK6CDI/HS8NlWEs4Vi12d6OA5ZsubX2ObXDY98EJ/HqEFySex\r\nwqb4qvlA/dOjkytWem1jp+/FkZDDnn1Vb7EVaepQo5DCpRT317Ms/Annjdlc\r\nG24821TnzibMm4Jj0BB6bwrH+N2mu3IxSpShZVWKAjVb/WRdKyu3HtjcMnOW\r\n0rXu50bmaDzXHkUlnXDImcA9zl/K/6uJD+LDeFKZ/36QOgF6CKIjbSSlgN5O\r\nZFJwZYsjXPbMRijo56RbLElzDrj5g8Kc75IZlD0B46Ni27VIbnd91++AAOP4\r\nQmEDhxOl/vgo+PKJ7QZ9aLyGWggrHcnKHwiQjaSncCYEczJwzv/+GQar0c+H\r\njUAzS41Wgvr8Gu5D3BMzyi3WnEOKNDqfpYa+a9ep+3aQn+ZGXBkycHgK2S7z\r\nJbM+iucLN08E/cp994tpy98Xt9hqSolShegBFrC3gKVfUJTnqP7MCHPdiDiC\r\nxkBWhfonkBgwZ3+UckGlnrz0KlMiwJOYxxq+gQnzVZutV4btS42z8bPkVFwc\r\nxwglSS5X4LRK7E9Z6HKGJvsZBJhV0+0K2j8=\r\n=amn8\r\n-----END PGP SIGNATURE-----\r\n"},"_npmUser":{"name":"segunadebayo","email":"joseshegs@gmail.com